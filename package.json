{
  "name": "ts-gaussian",
  "version": "0.0.0-placeholder",
  "description": "A TypeScript model of a Gaussian distribution",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/ts-gaussian",
  "homepage": "https://ts-gaussian.netlify.com",
  "keywords": [
    "typescript",
    "gaussian",
    "normal",
    "distribution"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "module": "./dist/module/index.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "lint": "eslint --ext .ts,.js .",
    "lint:fix": "eslint --ext .ts,.js --fix .",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.module.json",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src && touch docs/.nojekyll",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "npm run test -- --coverage --no-cache",
    "semantic-release": "semantic-release"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "25.1.4",
    "@types/node": "13.9.0",
    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "2.18.0",
    "eslint": "6.8.0",
    "eslint-config-xo-space": "0.22.0",
    "eslint-config-xo-typescript": "0.24.1",
    "eslint-plugin-import": "2.20.2",
    "jest": "25.1.0",
    "rimraf": "3.0.1",
    "semantic-release": "17.0.4",
    "ts-jest": "25.1.0",
    "typedoc": "0.16.9",
    "typescript": "3.7.5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
