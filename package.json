{
  "name": "ts-gaussian",
  "version": "0.0.0-placeholder",
  "description": "A TypeScript model of a Gaussian distribution",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/ts-gaussian",
  "keywords": [
    "typescript",
    "gaussian",
    "normal",
    "distribution"
  ],
  "source": "src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.m.js",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/*"
  ],
  "scripts": {
    "lint": "tslint -p tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "lint:fix": "tslint -p tsconfig.json -t codeFrame --fix 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "microbundle build --compress false --name=gaussian --format cjs,es --target node",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src && touch docs/.nojekyll",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run test -- --coverage --no-cache",
    "commit": "git-cz",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "23.3.1",
    "@types/node": "10.9.4",
    "commitizen": "2.10.1",
    "cz-conventional-changelog": "2.1.0",
    "jest": "23.5.0",
    "microbundle": "0.6.0",
    "rimraf": "2.6.2",
    "semantic-release": "15.9.12",
    "travis-deploy-once": "5.0.7",
    "ts-jest": "23.1.4",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-config-standard": "7.1.0",
    "typedoc": "0.12.0",
    "typescript": "3.0.1"
  },
  "release": {
    "branch": "master"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "testURL": "http://localhost",
    "transform": {
      ".ts": "ts-jest"
    },
    "testRegex": "(/test/.*|\\.(spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    },
    "collectCoverage": true
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
