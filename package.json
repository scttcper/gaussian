{
  "name": "ts-gaussian",
  "version": "0.0.0-placeholder",
  "description": "A TypeScript model of a Gaussian distribution",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/ts-gaussian",
  "keywords": [
    "typescript",
    "gaussian",
    "normal",
    "distribution"
  ],
  "source": "src/index.ts",
  "main": "./ts-gaussian.js",
  "module": "./ts-gaussian.m.js",
  "typings": "./src/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "lint": "tslint -p tsconfig.json -t codeFrame",
    "lint:fix": "tslint -p tsconfig.json -t codeFrame --fix",
    "prebuild": "rimraf dist",
    "build": "microbundle build --target node --format cjs,es -o dist",
    "postbuild": "cp package.json dist && cp LICENSE dist && cp README.md dist && rimraf dist/test",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src && touch docs/.nojekyll",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "npm run test -- --coverage --no-cache",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "23.3.13",
    "@types/node": "10.12.19",
    "jest": "23.6.0",
    "microbundle": "0.8.4",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.2",
    "ts-jest": "23.10.5",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.17.0",
    "tslint-xo": "0.10.0",
    "typedoc": "0.13.0",
    "typescript": "3.2.2"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
