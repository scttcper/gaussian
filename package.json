{
  "name": "ts-gaussian",
  "version": "0.0.0-placeholder",
  "description": "A TypeScript model of a Gaussian distribution",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/ts-gaussian",
  "keywords": [
    "typescript",
    "gaussian",
    "normal",
    "distribution"
  ],
  "source": "src/index.ts",
  "main": "./ts-gaussian.js",
  "module": "./ts-gaussian.m.js",
  "typings": "./src/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "lint": "tslint -p tsconfig.json -t codeFrame",
    "lint:fix": "tslint -p tsconfig.json -t codeFrame --fix",
    "prebuild": "rimraf dist",
    "build": "microbundle build --compress false --format cjs,es --target node -o dist",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src && touch docs/.nojekyll",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run test -- --coverage --no-cache",
    "commit": "git-cz",
    "travis-deploy-once": "travis-deploy-once",
    "presemantic-release": "cp package.json dist && cp LICENSE dist && cp README.md dist && rimraf dist/test",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "23.3.10",
    "@types/node": "10.11.7",
    "commitizen": "3.0.4",
    "cz-conventional-changelog": "2.1.0",
    "jest": "23.6.0",
    "microbundle": "0.6.0",
    "rimraf": "2.6.2",
    "semantic-release": "15.9.17",
    "travis-deploy-once": "5.0.9",
    "ts-jest": "23.10.4",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-xo": "0.9.0",
    "typedoc": "0.13.0",
    "typescript": "3.1.2"
  },
  "release": {
    "branch": "master"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
