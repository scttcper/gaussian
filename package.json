{
  "name": "ts-gaussian",
  "version": "0.0.0-placeholder",
  "description": "A TypeScript model of a Gaussian distribution",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/ts-gaussian",
  "homepage": "https://ts-gaussian.netlify.com",
  "keywords": [
    "typescript",
    "gaussian",
    "normal",
    "distribution"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "module": "./dist/module/index.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "lint": "eslint --ext .ts,.js .",
    "lint:fix": "eslint --ext .ts,.js --fix .",
    "prepare": "npm run build",
    "build": "del-cli dist && tsc -p tsconfig.build.json && tsc -p tsconfig.module.json",
    "build:docs": "typedoc --out docs --hideGenerator --target ES6 --mode file src && touch docs/.nojekyll",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "npm run test -- --coverage --no-cache",
    "semantic-release": "semantic-release"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "25.2.3",
    "@types/node": "13.13.12",
    "@typescript-eslint/eslint-plugin": "2.30.0",
    "@typescript-eslint/parser": "2.30.0",
    "del-cli": "3.0.1",
    "eslint": "6.8.0",
    "eslint-config-xo-space": "0.24.0",
    "eslint-config-xo-typescript": "0.28.0",
    "eslint-plugin-import": "2.22.0",
    "jest": "25.5.3",
    "semantic-release": "17.0.7",
    "ts-jest": "25.4.0",
    "typedoc": "0.17.6",
    "typescript": "3.8.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
